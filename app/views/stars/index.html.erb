<div class="container">
  <div class="row">
    <div class="col-sm-12 col-lg-12">
      <div class="d-flex flex-wrap justify-content-center">
        <% @users.each do |u| %>
          <% unless u.professor %>
            <div class="d-flex flex-column m-2 mb-0 align-items-center">
              <%= cl_image_tag u.photo.key, gravity: :face, crop: :thumb, width: 60, height: 60, radius: :max, crop: :fill, class: "avatar-large align-self-center" %>
              <%= u.nome_completo.split(" ")[0].titleize %>
              <i class="far fa-star star-index"></i>
            </div>
          <% end %>
        <% end %>
      </div>
      <%= render 'shared/footer' %>
    </div>
  </div>
</div>
