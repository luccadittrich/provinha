<div class="container">
  <div class="row">
    <div class="col-6">
      <% @users.each do |u| %>
        <div class="d-flex">
          <% unless u.professor %>
            <div class="w-10">
              <%= cl_image_tag u.photo.key, class: "avatar-large" %>
              <%= u.nome_completo %>
            </div>
          <% end %>
        </div>
      <% end %>
      <%= render 'shared/footer' %>
    </div>
  </div>
</div>
