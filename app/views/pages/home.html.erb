<div class="container">
  <div class="row">
    <div class="col-12 col-md-6 col-lg-4">
      <h3><%= "Conteudo da semana" %></h3>
      <% if current_user.professor %>
        <%= link_to 'Adicionar Conteúdo', new_post_path  %>
        <p>Corrigir</p>
      <% end %>
      <p>Questionários</p>
      <table>
        <th>Semana</th>
        <th>Questionário</th>
        <th>Opções</th>
        <% i = 1 %>
        <% 14.times do %>
          <tr>
            <td><%= i += 1 %></td>
            <td><%= link_to "Questionário #{i}", "#" %></td>
            <td>
              <% if current_user.professor %>
                <i class="fas fa-unlock-alt"></i></i>
                <i class="fas fa-lock"></i>
                <i class="fas fa-edit"></i>
              <% else %>
                <button class="btn btn-primary">Entrar</button>
              <% end %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
